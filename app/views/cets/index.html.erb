<p id="notice"><%= notice %></p>

<h1>Cets</h1>

<table>
  <thead>
    <tr>
      <th>Exercise</th>
      <th>Repetition</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cets.each do |cet| %>
      <tr>
        <td><%= cet.exercise %></td>
        <td><%= cet.repetition %></td>
        <td><%= cet.weight %></td>
        <td><%= link_to 'Show', cet %></td>
        <td><%= link_to 'Edit', edit_cet_path(cet) %></td>
        <td><%= link_to 'Destroy', cet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cet', new_cet_path %>
